{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "parser":"babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types":"off",
        "no-alert":1,
        "no-console":1,
        "eqeqeq":1,
        "semi":[1,"always"],
        "no-multi-spaces":1
    }
}
